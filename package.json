{
  "name": "vizjj",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tauri": "tauri",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen": "cd src-tauri && cargo test export_bindings && bun run fmt",
    "test": "cd src-tauri && cargo test",
    "clippy": "cd src-tauri && cargo clippy --fix --all-targets --allow-dirty",
    "fix": "bun run fmt && bun run clippy",
    "fmt": "bun run fmt:frontend && bun run fmt:backend",
    "fmt:frontend": "bun x prettier --write \"**/*.{js,ts,tsx,json,css,md,yml}\"",
    "fmt:backend": "cd src-tauri && cargo fmt",
    "fmt:ci:frontend": "bun x prettier --check \"**/*.{js,ts,tsx,json,css,md,yml}\"",
    "fmt:ci:backend": "cd src-tauri && cargo fmt --check",
    "clippy:ci": "cd src-tauri && cargo clippy -- -D warnings"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-log": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-store": "~2",
    "@tauri-apps/plugin-window-state": "~2",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.555.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.3.5",
    "react-resizable-panels": "^3.0.6",
    "react-virtuoso": "^4.15.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
